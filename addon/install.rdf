<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
        xmlns:em="http://www.mozilla.org/2004/em-rdf#">

    <Description about="urn:mozilla:install-manifest">
        <em:id>__addonID__</em:id>
        <em:name>__addonName__</em:name>
        <em:version>__buildVersion__</em:version>
        <em:multiprocessCompatible>true</em:multiprocessCompatible>
        <em:updateURL>__updaterdf__</em:updateURL>
        <em:creator>__author__</em:creator>
        <em:optionsURL>chrome://bamboo/content/preferences.xul</em:optionsURL>
        <em:type>2</em:type> <!-- type=extension -->
        <em:iconURL>chrome://bamboo/skin/favicon@0.5x.png</em:iconURL>
        <em:icon64URL>chrome://bamboo/skin/favicon.png</em:icon64URL>
        <em:bootstrap>true</em:bootstrap>

        <em:localized>
            <Description>
                <em:locale>en-US</em:locale>
                <em:name>__addonName__</em:name>
                <em:description>__descriptionenUS__</em:description>
            </Description>
        </em:localized>
        <em:localized>
            <Description>
                <em:locale>zh-CN</em:locale>
                <em:name>__addonName__</em:name>
                <em:description>__descriptionzhCN__</em:description>
            </Description>
        </em:localized>

        <!-- Or requires Zotero Standalone -->
        <em:targetApplication>
            <Description>
                <em:id>zotero@chnm.gmu.edu</em:id>
                <em:minVersion>5.*</em:minVersion>
                <em:maxVersion>*</em:maxVersion>
            </Description>
        </em:targetApplication>
        <!-- Or requires Juris-M -->
        <em:targetApplication>
          <Description>
            <em:id>juris-m@juris-m.github.io</em:id>
            <em:minVersion>4.0</em:minVersion>
            <em:maxVersion>*</em:maxVersion>
          </Description>
        </em:targetApplication>
    </Description>
</RDF>
